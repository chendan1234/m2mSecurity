CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES = YES
CLANG_WARN_QUOTED_INCLUDE_IN_FRAMEWORK_HEADER = NO
CONFIGURATION_BUILD_DIR = ${PODS_CONFIGURATION_BUILD_DIR}/TuyaSmartBLEMeshKit
DEFINES_MODULE = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/CocoaAsyncSocket" "${PODS_CONFIGURATION_BUILD_DIR}/YYModel" "${PODS_ROOT}/TYBluetooth/ios" "${PODS_ROOT}/TuyaSmartActivatorCoreKit/ios" "${PODS_ROOT}/TuyaSmartBLEKit/Build" "${PODS_ROOT}/TuyaSmartBLEMeshKit/Build" "${PODS_ROOT}/TuyaSmartBaseKit/ios" "${PODS_ROOT}/TuyaSmartDeviceCoreKit/ios" "${PODS_ROOT}/TuyaSmartMQTTChannelKit/ios" "${PODS_ROOT}/TuyaSmartQUIC/Carthage/Build/iOS" "${PODS_ROOT}/TuyaSmartSocketChannelKit/ios" "${PODS_ROOT}/TuyaSmartUtil/Carthage/Build/iOS" "${PODS_XCFRAMEWORKS_BUILD_DIR}/TuyaSmartBLEKit" "${PODS_XCFRAMEWORKS_BUILD_DIR}/TuyaSmartBLEMeshKit"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
OTHER_LDFLAGS = $(inherited) -framework "CoreBluetooth" -framework "CoreData" -framework "CoreTelephony" -framework "Foundation" -framework "Security" -framework "SystemConfiguration" -framework "TuyaSmartQUIC" -framework "TuyaSmartUtil" -framework "UIKit"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}
PODS_TARGET_SRCROOT = ${PODS_ROOT}/TuyaSmartBLEMeshKit
PODS_XCFRAMEWORKS_BUILD_DIR = $(PODS_CONFIGURATION_BUILD_DIR)/XCFrameworkIntermediates
PRODUCT_BUNDLE_IDENTIFIER = org.cocoapods.${PRODUCT_NAME:rfc1034identifier}
SKIP_INSTALL = YES
TUYA_PRIVACY_USAGE_DESCRIPTION = NSBluetoothAlwaysUsageDescription NSBluetoothPeripheralUsageDescription
USE_RECURSIVE_SCRIPT_INPUTS_IN_SCRIPT_PHASES = YES
